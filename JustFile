build:
        GOOS=js GOARCH=wasm go build -C ./src/ -o ../wasm/main.wasm

run:
        GOOS=js GOARCH=wasm go run ./src/ 

test:
        GOOS=js GOARCH=wasm go test ./src/ 

dev:
        just build
        bun run dev

lint-fmt:
        GOOS=js GOARCH=wasm golangci-lint run
        GOOS=js GOARCH=wasm golangci-lint fmt
